{
  "name": "icbm",
  "version": "0.4.1",
  "description": "Interactive Configurations Backup Manager",
  "repository": {
    "url": "https://github.com/kimxogus/icbm.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/kimxogus/icbm/issues/"
  },
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "author": {
    "name": "Taehyun Kim",
    "email": "kgyoo8232@gmail.com",
    "url": "https://github.com/kimxogus"
  },
  "license": "MIT",
  "bin": {
    "icbm": "bin/icbm.js"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "cross-env NODE_ENV=test babel-node scripts/test",
    "prebuild": "npm run lint && babel-node scripts/clearBuild.js",
    "build:prod": "rimraf build/* && cross-env NODE_EMV=production babel src -d build --ignore **/__tests__/*"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix .",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.84.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.4.4",
    "prettier-eslint": "^8.4.0",
    "remove": "^0.1.5",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@octokit/rest": "^15.15.1",
    "chalk": "^2.3.0",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "fs-extra": "^7.0.0",
    "get-env-var": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "left-pad": "^1.1.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "rc": "^1.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
