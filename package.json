{
  "name": "icbm",
  "version": "0.3.2",
  "description": "Interactive Configurations Backup Manager",
  "repository": {
    "url": "https://github.com/kimxogus/icbm.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/kimxogus/icbm/issues/"
  },
  "engines": {
    "node": ">=4"
  },
  "preferGlobal": true,
  "author": {
    "name": "Taehyun Kim",
    "email": "kgyoo8232@gmail.com",
    "url": "https://github.com/kimxogus"
  },
  "license": "MIT",
  "bin": {
    "icbm": "bin/icbm.js"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "cross-env NODE_ENV=test babel-node scripts/test",
    "prebuild": "npm run lint && babel-node scripts/clearBuild.js",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_EMV=production babel src -d build --ignore **/__tests__/*",
    "prepublish": "yarn build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix .",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.0.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-prettier": "^2.1.1",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.4.4",
    "prettier-eslint": "^8.4.0",
    "remove": "^0.1.5"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "chalk": "^2.3.0",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "fs-extra": "^5.0.0",
    "get-env-var": "^2.0.0",
    "github": "^13.0.1",
    "json-stable-stringify": "^1.0.1",
    "left-pad": "^1.1.3",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "rc": "^1.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ]
  }
}
